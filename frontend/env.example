# Frontend Environment Configuration
# Copy this file to .env and update the values

# API Configuration
REACT_APP_API_URL=http://127.0.0.1:8000

# Environment
NODE_ENV=development

# Features
REACT_APP_ENABLE_AUTH=true
REACT_APP_ENABLE_BOOKING=true
REACT_APP_ENABLE_MULTILINGUAL=true

# UI Configuration
REACT_APP_THEME=light
REACT_APP_PRIMARY_COLOR=#3B82F6
REACT_APP_SECONDARY_COLOR=#10B981

# WebSocket Configuration
REACT_APP_WEBSOCKET_ENABLED=false
REACT_APP_WEBSOCKET_URL=ws://127.0.0.1:8001/ws/

# Polling Configuration
REACT_APP_POLLING_ENABLED=true
REACT_APP_POLLING_INTERVAL=5000

# Error Handling
REACT_APP_MAX_RETRIES=3
REACT_APP_RETRY_DELAY=1000









