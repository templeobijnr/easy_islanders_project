{
  "dashboard": {
    "id": null,
    "title": "Easy Islanders - Router Performance",
    "tags": ["easy-islanders", "router", "ml"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "Router Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(router_requests_total[5m])",
            "legendFormat": "{{status}} - {{domain}}"
          }
        ],
        "yAxes": [
          {
            "unit": "reqps",
            "min": 0
          }
        ]
      },
      {
        "id": 2,
        "title": "Router Confidence Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(router_confidence_histogram_bucket[5m])",
            "legendFormat": "{{domain}} - {{le}}"
          }
        ]
      },
      {
        "id": 3,
        "title": "Expected Calibration Error (ECE)",
        "type": "graph",
        "targets": [
          {
            "expr": "router_calibration_ece",
            "legendFormat": "{{domain}}"
          }
        ],
        "thresholds": [
          {
            "value": 0.05,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      },
      {
        "id": 4,
        "title": "Router Accuracy",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(router_accuracy_total{correct=\"true\"}[5m]) / rate(router_accuracy_total[5m])",
            "legendFormat": "{{domain}}"
          }
        ],
        "yAxes": [
          {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        ]
      },
      {
        "id": 5,
        "title": "Router Latency P95",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(router_latency_seconds_bucket[5m]))",
            "legendFormat": "{{domain}}"
          }
        ],
        "yAxes": [
          {
            "unit": "seconds",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 0.12,
            "colorMode": "warning",
            "op": "gt"
          }
        ]
      },
      {
        "id": 6,
        "title": "Uncertain Predictions Ratio",
        "type": "graph",
        "targets": [
          {
            "expr": "router_uncertain_ratio",
            "legendFormat": "{{domain}}"
          }
        ],
        "yAxes": [
          {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        ]
      },
      {
        "id": 7,
        "title": "Active WebSocket Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(websocket_connections_active)",
            "legendFormat": "connections"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 0
          }
        ]
      },
      {
        "id": 8,
        "title": "WS Send Errors (5m)",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(ws_message_send_errors_total[5m])",
            "legendFormat": "errors"
          }
        ],
        "yAxes": [
          {
            "unit": "short",
            "min": 0
          }
        ],
        "thresholds": [
          {
            "value": 0,
            "colorMode": "critical",
            "op": "gt"
          }
        ]
      }
      ,
      {
        "id": 21,
        "title": "Prefs Extract Requests (rate)",
        "type": "graph",
        "targets": [
          { "expr": "rate(prefs_extract_requests_total[5m])", "legendFormat": "{{result}}" }
        ],
        "yAxes": [ { "unit": "reqps", "min": 0 } ]
      },
      {
        "id": 22,
        "title": "Prefs Saved (5m)",
        "type": "graph",
        "targets": [
          { "expr": "increase(prefs_saved_total[5m])", "legendFormat": "{{category}}/{{type}}/{{source}}" }
        ],
        "yAxes": [ { "unit": "short", "min": 0 } ]
      },
      {
        "id": 23,
        "title": "Prefs Applied (5m)",
        "type": "graph",
        "targets": [
          { "expr": "increase(prefs_applied_total[5m])", "legendFormat": "{{agent}}" }
        ],
        "yAxes": [ { "unit": "short", "min": 0 } ]
      },
      {
        "id": 24,
        "title": "Prefs Contradictions (5m)",
        "type": "graph",
        "targets": [
          { "expr": "increase(prefs_contradictions_total[5m])", "legendFormat": "{{type}}" }
        ],
        "yAxes": [ { "unit": "short", "min": 0 } ]
      },
      {
        "id": 25,
        "title": "Prefs Extraction Latency P95",
        "type": "graph",
        "targets": [
          { "expr": "histogram_quantile(0.95, rate(prefs_latency_seconds_bucket[5m]))", "legendFormat": "p95" }
        ],
        "yAxes": [ { "unit": "seconds", "min": 0 } ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
